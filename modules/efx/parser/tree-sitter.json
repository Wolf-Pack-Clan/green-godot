{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "efx",
      "camelcase": "EFX",
      "title": "EFX Parser",
      "scope": "source.efx",
      "file-types": [
        "efx",
        "text/plain",
        "text/x-efx"
      ],
      "injection-regex": "^efx$",
      "class-name": "TreeSitterEfx"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GNU GPLv3",
    "description": "Green Godot effect file parser",
    "authors": [
      {
        "name": "Kazam",
        "email": "mrcreamy112233@gmail.com",
        "url": "https://gitlab.com/kazam0180"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-efx"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}